<UserControl x:Class="Transfer.Control.ItemDictionaryControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Transfer.Control"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem>
                <TabItem.Header>
                    <TextBlock>項目辞書</TextBlock>
                </TabItem.Header>
                <local:DictionaryControl x:Name="_dictCtrl"/>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <TextBlock>形態素辞書</TextBlock>
                </TabItem.Header>
                <local:MorphControl x:Name="_morphCtrl"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
